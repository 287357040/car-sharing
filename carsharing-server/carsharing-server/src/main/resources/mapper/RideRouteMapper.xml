<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.carsharing.server.mapper.RideRouteMapper">
    <resultMap id="BaseResultMap" type="com.carsharing.server.entity.User">
        <id column="user_num" property="userNo" jdbcType="VARCHAR" />
        <result column="user_name" property="userName" jdbcType="VARCHAR" />
        <result column="password" property="password" jdbcType="VARCHAR" />
        <result column="sex" property="sex" jdbcType="TINYINT" />
        <result column="birth" property="birth" jdbcType="DATE" />
        <result column="department" property="department" jdbcType="VARCHAR" />
        <result column="is_driver" property="isDriver" jdbcType="TINYINT" />
        <result column="mobile" property="mobile" jdbcType="VARCHAR" />
        <result column="create_time" property="createTime" jdbcType="TIMESTAMP" />
        <!--<result column="state" property="state" jdbcType="TINYINT" />-->
        <result column="update_time" property="updateTime" jdbcType="TIMESTAMP" />
    </resultMap>
    <sql id="Base_Column_List">
        user_num, user_name,password, sex,birth, department,is_driver,mobile, create_time
        ,update_time
    </sql>
    <select id="selectByPrimaryKey" resultMap="BaseResultMap"
            parameterType="java.lang.String">
        select
        <include refid="Base_Column_List" />
        from t_user
        where user_num = #{id,jdbcType=VARCHAR}
    </select>
</mapper>